{
  "name": "reactstatic-wordpress-template",
  "version": "3.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start:rs": "dotenv react-static start",
    "build": "dotenv react-static build",
    "serve": "serve dist -p 3000",
    "start:lambda": "netlify-lambda serve src/lambda",
    "build:lambda": "netlify-lambda build src/lambda",
    "start": "concurrently --kill-others-on-fail \"npm run start:lambda\" \"npm run start:rs\"",
    "typecheck": "flow-watch",
    "test": "jest --watch",
    "export": "bit tag --all && bit export seamonster-studios.react",
    "include": "yarn run include:components && yarn run include:utils",
    "include:components": "bit add src/components/* -t 'src/__tests__/{PARENT}.spec.js'",
    "include:utils": "bit add src/utils/* -t 'src/__tests__/{PARENT}.spec.js'",
    "add:component": "node package.sms.bit.js add components",
    "add:util": "node package.sms.bit.js add utils",
    "import:component": "node package.sms.bit.js import components",
    "import:util": "node package.sms.bit.js import utils"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "always"
  },
  "quokka": {
    "babel": true,
    "plugins": [
      "jsdom-quokka-plugin"
    ],
    "env": {
      "params": {
        "env": "BABEL_ENV=test"
      }
    }
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.55",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.55",
    "@babel/plugin-proposal-json-strings": "^7.0.0-beta.55",
    "@babel/plugin-syntax-import-meta": "^7.0.0-beta.55",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0-beta.54",
    "@bit/seamonster-studios.react.components.error-boundary": "^1.0.4",
    "@bit/seamonster-studios.react.utils.styles": "^0.0.2",
    "@sentry/browser": "^4.0.2",
    "axios": "^0.17.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^22.4.4",
    "babel-plugin-emotion": "^9.2.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^3.6.1",
    "dotenv": "^5.0.1",
    "dotenv-cli": "^1.4.0",
    "emotion": "^9.2.5",
    "eslint-plugin-babel": "^5.2.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-react": "^7.10.0",
    "flow-bin": "^0.81.0",
    "flow-typed": "^2.5.1",
    "flow-watch": "^1.1.3",
    "he": "^1.1.1",
    "jest": "^22.4.4",
    "moment": "^2.22.2",
    "netlify-lambda": "^0.4.0",
    "raven-js": "^3.26.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-emotion": "^9.2.5",
    "react-hot-loader": "^4.3.8",
    "react-spring": "^5.8.0",
    "react-static": "^6.0.0-beta.21",
    "react-static-plugin-emotion": "^1.0.6",
    "react-static-routes": "^1.0.0",
    "reset-css": "^4.0.1",
    "to-case": "^2.0.0",
    "webpack": "^4.16.3"
  },
  "devDependencies": {
    "@babel/preset-flow": "^7.0.0",
    "eslint": "^5.6.0",
    "eslint-config-react-tools": "^1.2.5",
    "flow": "^0.2.3",
    "jsdom-quokka-plugin": "^1.0.13",
    "serve": "^9.4.0"
  }
}
