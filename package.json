{
  "name": "reactstatic-seamonster-template",
  "version": "3.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start:rs": "dotenv react-static start",
    "build": "dotenv react-static build",
    "serve": "serve dist -p 3100",
    "start:lambda": "netlify-lambda serve src/lambda",
    "build:lambda": "netlify-lambda build src/lambda",
    "start": "concurrently --kill-others-on-fail \"npm run start:lambda\" \"npm run start:rs\"",
    "test": "jest --watch",
    "ss:tag": "node package.sms.js tag",
    "ss:add": "node package.sms.js yarn-add",
    "ss:watch": "node package.sms.js-add",
    "ss:import": "node package.sms.js import",
    "ss:export": "node package.sms.js export"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "always"
  },
  "quokka": {
    "babel": true,
    "plugins": [
      "jsdom-quokka-plugin"
    ],
    "env": {
      "params": {
        "env": "BABEL_ENV=test"
      }
    }
  },
  "dependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@bit/seamonster-studios.react.components.error-boundary": "^1.0.4",
    "@bit/seamonster-studios.react.utils.styles": "^0.0.2",
    "@sentry/browser": "^4.0.5",
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-emotion": "^9.2.10",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^4.0.1",
    "dotenv": "^6.0.0",
    "dotenv-cli": "^1.4.0",
    "emotion": "^9.2.10",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-jest": "^21.22.1",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "moment": "^2.22.2",
    "netlify-lambda": "^0.4.0",
    "prop-types": "^15.6.2",
    "raven-js": "^3.27.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-emotion": "^9.2.10",
    "react-hot-loader": "^4.3.11",
    "react-spring": "^5.8.0",
    "react-static": "^6.0.0-beta.22",
    "react-static-plugin-emotion": "^1.0.6",
    "react-static-routes": "^1.0.0",
    "reset-css": "^4.0.1",
    "to-case": "^2.0.0",
    "webpack": "^4.20.2"
  },
  "devDependencies": {
    "eslint": "^5.6.0",
    "eslint-config-react-tools": "^1.1.6",
    "jsdom-quokka-plugin": "^1.0.13",
    "serve": "^10.0.2"
  }
}
